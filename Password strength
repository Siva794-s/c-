#include <stdio.h>
#include <string.h>

int main() {
    char password[100];
    int length;

    printf("Enter your password: ");
    scanf("%s", password);

    length = strlen(password);

    if (length >= 8) {
        printf("Password is Strong.\n");
    } else {
        printf("Password is Weak. It should be at least 8 characters long.\n");
    }

    return 0;
}
