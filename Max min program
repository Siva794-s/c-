#include <stdio.h>

int main() {
    int a, b, c, d, e;
    int max, min;

    printf("Enter 5 numbers:\n");
    scanf("%d %d %d %d %d", &a, &b, &c, &d, &e);

    max = a;  // Assume a is max
    if(b > max) max = b;
    if(c > max) max = c;
    if(d > max) max = d;
    if(e > max) max = e;

    min = a;  // Assume a is min
    if(b < min) min = b;
    if(c < min) min = c;
    if(d < min) min = d;
    if(e < min) min = e;

    printf("Maximum number is: %d\n", max);
    printf("Minimum number is: %d\n", min);

    return 0;
}
